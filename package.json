{
  "name": "vocab-memorizer-backend",
  "version": "1.0.0",
  "description": "Vocabulary Memorizer Backend",
  "main": "src/app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/app.js",
    "migrate": "npx sequelize db:migrate --migration migrations/create-tables.js",
    "seed": "npx sequelize db:seed --seed seeders/initial-data.js",
    "init": "npm run migrate && npm run seed",
    "dev": "cross-env NODE_ENV=development nodemon src/app.js"
  },
  "author": "Ze-Yuan, Duan",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.45",
    "mysql2": "^3.10.0",
    "node-cron": "^3.0.3",
    "p-queue": "^8.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.14",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "url": "^0.11.3",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "babel-plugin-transform-commonjs": "^1.1.6",
    "cross-env": "^7.0.3",
